/* pages/financialObjective/financialObjective.wxss */

.fin-boj{
    width: 700rpx;
    margin: 0 auto;
}
.fin-boj .tit{
    margin: 50rpx auto;
    font-size:16px;
    display: block;
}

.fin-boj .finance{
    margin: 0 auto 100rpx;
    display: block;
    text-align: center;
}
.fin-boj .finance text:first-of-type{
  margin-right: 50rpx;
}


.fin-boj .sel-btn{
  width: 500rpx;
  margin: 100rpx auto;
   overflow: auto;
}

.fin-boj .sel-btn button{
   width: 40%;
}

.fin-boj .sel-btn .again{
 float: left;
 background: #5fc4f3;
 color: #fff;
}
.fin-boj .sel-btn .share{
 float: right;
}

.fin-boj  .test{
  margin: 100px auto;
  border: 1px solid #ccc;
  font-size: 28rpx;
  width:500rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  border-radius: 10rpx;
  display: block;
}